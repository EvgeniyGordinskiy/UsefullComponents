.imageViewer{
  display: flex;
  justify-content: center;
  align-self: center;
  flex-direction: column;
  .modal-container{
    height: 100%;
    width: 100%;
    max-width: 95%;
    max-height: 70%;
    background transparent
    box-shadow none;
    .modal-body{
      height: 100%;
      width: 100%;
      .el-carousel {
        height: 100%;
        width: 100%;
        max-width 80%;
        max-height 90%;
        min-width 250px;
        min-height 80px;
        margin:0 auto;
        -webkit-transition: all 0.25s ease-in-out;
        -moz-transition: all 0.25s ease-in-out;
        -o-transition: all 0.25s ease-in-out;
        -ms-transition: all 0.25s ease-in-out;
        .el-carousel__container {
          height: 100%;
          width: 100%;
          .el-carousel__item {
            height: 100%;
            width: 100%;
            font-size 0;
            img{
              max-width:100%;
              max-height:100%;
            }
          }
        }

    }
    }
  }
}